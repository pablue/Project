<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping 
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.edu.xyc.dm.model.Student" table="dm_student">
		<id name="sid" type="java.lang.Long" length="5">
			<generator class="increment"></generator>
		</id>
		<property name="sname" length="30"></property>
		<property name="sno" length="30"></property>
		<property name="dorm" length="30"></property>
		<property name="sex" length="4"></property>
		<property name="tel" length="12"></property>
		<many-to-one name="banji" class="cn.edu.xyc.dm.model.Banji"
			fetch="join"></many-to-one>
	</class>
</hibernate-mapping>